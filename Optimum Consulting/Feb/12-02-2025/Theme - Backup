        });

        $(".sticky-header .menu-item-10904").hover(function () {
            $(".sticky-header .menu-item-10904 .submenu").slideToggle(250);
        });
    });

    $(window).resize(function () {
        if ($(window).width() < 991) {
            $(".main-header li.menu-item-has-children").unbind();
        } else {
        }
    });

    $( window ).on("load", function() {
        if ($(window).width() < 991) {
            $(".main-header li.menu-item-has-children").unbind();
        }
    });
</script>

<script>
    $(window).scroll(function () {
        var scroll = $(window).scrollTop();

        if (scroll >= 300) {
            $(".main-header").addClass("darkHeader");
        } else {
            $(".main-header").removeClass("darkHeader");
        }
    });
</script>

<script>
    $(document).ready(function () {
        $("#dropdownMenu3").click(function () {
            $(".dropdown").toggleClass("active");
            $(".dropdown-menu").toggleClass("active");
        });
    });
</script>

<script>
    $(document).mouseup(function (e) {
        var container = $(".pull-right .form-container .form-group input");

        var containertwo = $(".search-box-outer #dropdownMenu3");

        if (!container.is(e.target) && container.has(e.target).length === 0 && !containertwo.is(e.target) && containertwo.has(e.target).length === 0) {
            $(".search-box-outer .pull-right").removeClass("active");

            $(".search-box-outer .dropdown").removeClass("active");

            $(".for-mobile").removeClass("active");
        }
    });
</script>

<script>
    $(".navbar-header .navbar-toggle").click(function () {
        $(".navbar-collapse.collapse").slideToggle();
    });

    $(".main-menu .navigation .dropdown-btn").click(function () {
        $(this).prev("ul").slideToggle();
    });
</script>

<!--Landing Page Update UI-->
<script>
$( window ).on("load", function() {
    if($('div').hasClass('applicationStepwelcomepage')){
        $('.main_container').addClass('menu-flex');
    }else{
        $('.main_container').removeClass('menu-flex');
    }
    
    if($('div').hasClass('thankyoupage')){
        $('.main_container').addClass('menu-flex-1');
    }else{
        $('.main_container').removeClass('menu-flex-1');
    }
});
$(document).ready(function(){
    var urlPartOne = window.location.href;
    urlPartOne = urlPartOne.split('.xyz/')[0];
    urlPartOne = urlPartOne + '.xyz/onboard/';
    var urlPartTwo = "{{applicationModel.organizationId}}";
    var urlPartThree = "/application/downloadall/";
    var urlPartFour = "{{applicationModel.id}}";
    var urlFull = urlPartOne + urlPartTwo + urlPartThree + urlPartFour;
    $(".download-all-button").attr("href",urlFull);
  });
</script>
<script>
$(document).ready(function(){
  $('#startButton').removeClass('processStep2');
  $('#applicationSkipButton').removeClass('processStep2');
  $('#applicationPrevButton').removeClass('processStep');
  $('#applicationSkipButtonNoSave').removeClass('processStep');
  
  $('#startButton').on('click', function() {
    $(this).prop('disabled', true);
    $(this).addClass('disableButton');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
  
  $('#applicationSkipButton').on('click', function() {
    $(this).prop('disabled', true);
    $(this).addClass('disableButton');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
  
  $('.skip-button').on('click', function() {
    $(this).prop('disabled', true);
    $(this).addClass('disableButton');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
  
  $('#applicationPrevButton').on('click', function() {
    $(this).prop('disabled', true);
    $(this).addClass('disableButtonPrevSkip');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
  
  $('#applicationSkipButtonNoSave').on('click', function() {
    $(this).prop('disabled', true);
    $(this).addClass('disableButtonPrevSkip');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
  $('.new-welcome-uri').on('click', function() {
    $(this).prop('disabled', true);    
    $(this).addClass('disableButton');
    $(this).css('pointer-events', 'none');
    $(this).html('Please Wait...');
    $(this).css('opacity', '0.65');
  });
});
</script>
