<script>
$(".skip-button").click(function (event) {
    event.preventDefault();
    $("#applicationForm input.form-control").val("");
    $("#applicationForm .filter-option .filter-option-inner-inner").text("* Select Country");
    var skipuri = $(".skip-button").attr("href");
    window.location.href = skipuri;
});
</script>

<script>
$(document).ready(function() {
    var ua = window.navigator.userAgent;
    var isIE = /MSIE|Trident/.test(ua);

    if (isIE) {
        $('.progress-bar').addClass('internetexplorer');
    } else {
        $('.progress-bar').removeClass('internetexplorer');
    }
});
</script>


<script>
$(document).ready(function(){
    $('div#alertModal button.btn.btn-primary').click(function(){
    $('div#alertModal').modal('hide');
});
});
</script>  

 
<script>
$(document).ready(function(){
    var cutsave = window.location.href;
    cutsave = cutsave.split('.xyz/')[0]; 
    cutsave = cutsave + '.xyz/onboard/public/application/';
    var url2 = "{{applicationModel.id}}";
    var ur3 = "/welcome";
    var urlmain = cutsave + url2 + ur3;
    $(".save-exit-button").attr("href",urlmain);

    if($(".Assignment-p:contains('!!!! MISSING !!!!')").length) {
        $('.Assignment-p').text("");
    }else{
            
    }
        
    var currentValue = $('.percentage').text();
    var goalValue = "1";
    var percentage = Math.floor(((currentValue / goalValue) * 100)) +"%";
    $(".percentage-two span").text(percentage);
    $(".embed-responsive-item").contents().find("h1").addClass("myClass");
    
    setInterval(function() {
        if(jQuery('#adobeSignForm').attr('value') =="complete") {
            $('.embed-responsive-4by3').addClass('is-active');
            $('.ratio.ratio-16x9').addClass('is-active');
        }else{
            $('.embed-responsive-4by3').removeClass('is-active');
            $('.ratio.ratio-16x9').removeClass('is-active');
        }        
     }, 3000);

    setInterval(function() {
        if($(".btn-save-con").hasClass("d-none")) {
            $('.btn-prev').addClass('top-up');
        }else{
            $('.btn-prev').removeClass('top-up');
        }
    }, 0000);
});
</script>
<script>
$(document).ready(function() {
    $('.form-field-label:contains("Workers Compensation Insurance")').addClass('upld-1');
    $('.upld-1').parents('div.form-group').addClass('form-group-one');
    $('.form-field-label:contains("Public Indemnity Insurance")').addClass('upld-2');
    $('.upld-2').parents('div.form-group').addClass('form-group-two');
    $('.form-field-label:contains("Public Liability Insurance")').addClass('upld-3');
    $('.upld-3').parents('div.form-group').addClass('form-group-three');
    setInterval(function() {
        if ($(".form-group-one input[type='hidden']").val() !== '') {
            if ($(".form-group-one .w-100 p strong:contains('No File Submitted')").length) {
                $('.form-group-one .w-100 p strong').text("");
            }
        }
        if ($(".form-group-two input[type='hidden']").val() !== '') {
            if ($(".form-group-two .w-100 p strong:contains('No File Submitted')").length) {
                $('.form-group-two .w-100 p strong').text("");
            }
        }
        if ($(".form-group-three input[type='hidden']").val() !== '') {
            if ($(".form-group-three .w-100 p strong:contains('No File Submitted')").length) {
                $('.form-group-three .w-100 p strong').text("");
            }
        }
    });
    if ($(".img-welcome").length > 0 || $(".thankyou-page").length > 0) {
        $('.newheader').addClass('remove-margin');
    } else {
        $('.newheader').removeClass('remove-margin');
    }
});
</script>

<script>
    $(".noclick").click(function (event) {
        event.preventDefault();
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {
        $(".sticky-header .navbar-collapse .navigation .menu-item.menu-item-has-children").removeAttr("id");

        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                $(".scroll-top").fadeIn();
            } else {
                $(".scroll-top").fadeOut();
            }
        });

        $(".scroll-top").click(function () {
            $("html, body").animate({ scrollTop: 0 }, 600);
            return false;
        });

    });
</script>

