<div class="main_container">
    <a href="#top" class="mmyclasstop"></a>
    {{#applicationModel.currentForm}}
    <div class="container">
        <div class="percentage">{{applicationModel.meta.progress}}</div>
        
        <div class="percentage-two">
            <div class="percent-sub-div">Completion %:<span></span></div>
        </div>
        
        <div class="save-and-exit">
            <a class="btn mr-1 save-exit-button"  id="applicationSkipButton" data-validate="false">Save & Exit</a>
        </div>
    </div>
  
    <script>
        var inprogress = 0;
    </script>
        
    {{#applicationModel.allForms}}        
        {{^completed}}
            <script>
               $(document).ready(function() {
                var tag = $('#applicationNextButton').attr("href");
                    $('#applicationNextButton').each(function() {
                        if (tag.indexOf('completed') > 0 ) {
                                $(this).addClass('last-btn');
                                var cut = window.location.href;
                                cut = cut.split('.xyz/')[0]; 
                                cut = cut + '.xyz/onboard/';
                                var onboardId = "{{applicationModel.id}}";
                                var organizationId = "{{applicationModel.organizationId}}";
                                var formId = "{{id}}";
                                var url = cut + organizationId + '/application/' + onboardId + '/' + formId;
                                $(".last-btn").attr("href",url);
                            }else {
                                      
                               }
                    });
            });
               var inprogress = 1;
               var notcm = '';
               if(notcm == ''){
                if(window.location.href.indexOf('#book') > 0) {
                    if(window.location.href.indexOf("#tag") == -1){
                        var cut = window.location.href;
                           cut = cut.split('.xyz/')[0]; 
                           cut = cut + '.xyz/onboard/';
                           var onboardId = "{{applicationModel.id}}";
                           var organizationId = "{{applicationModel.organizationId}}";
                           var formId = "{{id}}";
                           var url = cut + organizationId + '/application/' + onboardId + '/' + formId;
                        window.location.href = url + "#tag";
                        }            
                    }
            }
            </script>
        {{/completed}}
    {{/applicationModel.allForms}}
    
    <script>
    if(inprogress == 0){
        $(".overlay-fix").css("display", "block");        
        if(window.location.href.indexOf("#thanks") == -1){
            var cutthanks = window.location.href;
            cutthanks = cutthanks.split('.xyz/')[0]; 
            cutthanks = cutthanks + '.xyz/onboard/';
            var onboardIdThanks = "{{applicationModel.id}}";
            var organizationIdThanks = "{{applicationModel.organizationId}}";
            var formIdThanks = "{{id}}";
            var urlThanks = cutthanks + organizationIdThanks + '/application/' + onboardIdThanks  + '/completed';
            window.location.href = urlThanks + "#thanks";
        }
    }
    </script>
  
    <div class="progress-bar">
        <div class="nav nav-tabs step-anchor pro-ul">
            {{#applicationModel.allForms}}
                {{#current}}
                    {{#completed}}
                    <button id="step{{step}}" class="pro-li unticket active is-active" type="button"><a class="pro-li-a"></a></button> 
                    {{/completed}}
          
                    {{^completed}}
                    <button id="step{{step}}" class="pro-li unticket active" type="button"><a class="pro-li-a"></a></button> 
